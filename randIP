import random

IP="1.1.1.1"
used=[]

def getIP():
        IP=random.randint(1,255)
def checkIP():
        for x in range(len(used)):
                if used[x] == str(IP):
                        getIP()
                        checkIP()
                else
                        used.append(str(IP))
                        print str(IP)
getIP()
print str(IP)
checkIP()
